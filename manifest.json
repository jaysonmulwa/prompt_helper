{
  "manifest_version": 3,
  "name": "prompt helper",
  "description": "Enhance and organize your prompts for more effective results.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab"]
}
